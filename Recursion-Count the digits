import java.io.*;
import java.util.*;

public class Solution {

    static int count(int num){
        if(num<10){
            return 1;
        }
        return 1+count(num/10);
    }
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int num =sc.nextInt();
        System.out.println("The Count the digits: "+count(num));
    }
          
        
    }
