import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner scanner = new Scanner(System.in);
        String a= scanner.nextLine();
        int arrears = scanner.nextInt();
        int cgpa = scanner.nextInt();
        System.out.println("Name of the Student:" +a);
        if ((arrears == 1 && cgpa > 70) || ((arrears == 1 || arrears == 2) && cgpa > 75)) {
            System.out.println(a + " is Eligible for Placement");
        } else {
            System.out.println(a + " is Not Eligible for Placement");

    }
}
    }
